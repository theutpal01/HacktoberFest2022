def linear_Search(list1, n, no):  
  
    for i in range(0, n):  
        if (list1[i] == no):  
            return i  
    return -1  
  
  
list1 = [1 ,3, 5, 4, 7, 9]  
no = 7  
  
n = len(list1)  
res = linear_Search(list1, n, no)  
if(res == -1):  
    print("Element not found")  
else:  
    print("Element found at index: ", res) 
